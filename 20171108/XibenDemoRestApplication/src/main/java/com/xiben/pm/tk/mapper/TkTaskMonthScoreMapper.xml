  <insert id="insertAll">
		insert into tk_task_monthscore(compid,deptid,`year`,`month`,taskscore,`status`)
		select 
		t.compid,
		t.deptid,
		cast( DATE_FORMAT(t.publishdt,'%Y') as SIGNED   INTEGER) as year_,
		cast( DATE_FORMAT(t.publishdt,'%m') as SIGNED   INTEGER) as month_,
		sum(t.score) as monthscore_score,
		2 as monthscore_status
		from tk_task t
		where t.`status`=3
		group by t.compid, t.deptid,t.publishdt
 
